\[
\begin{tikzpicture}
    \node (1) at (-1,1) {\\( $1 \\)};
    \node (2) at (-1,-1) {\\( $2 \\)};
    \node (3) at (1,1) {\\( $3 \\)};
    \node (4) at (1,-1) {\\( $4 \\)};
    \draw [->] (1) to node [left] {\scriptsize{\\( $5 \\)}} (2);
    \draw [->] (1) to node [above] {\scriptsize{\\( $6 \\)}} (3);
    \draw [->] (2) to node [above] {\scriptsize{\\( $7 \\)}} (4);
    \draw [->] (3) to node [right] {\scriptsize{\\( $8 \\)}} (4); 
\end{tikzpicture}
