% body -------------------------

% question 1
\begin{q}[10 points]
  Let $ f(x)= x^2-4 $ and $ g(x)= x^3 $.  Evaluate the
  expression $ g(f(u)) $.  
\end{q}

% question 2
\begin{q}[10 points]
  Determine whether the graph of the following function is
  symmetric about the x-axis, y-axis or the origin:
  $ f(x)= x^4+5x^2-12 $. (For two extra credit points,
  describe why $ f(x) $ is not symmetric about the x-axis
  in one or two sentences and no calculations.)  
\end{q}

% end document------------------

