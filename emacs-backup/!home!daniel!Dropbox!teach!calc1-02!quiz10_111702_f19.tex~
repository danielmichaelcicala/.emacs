% body -------------------------

% question 1
\begin{q}[2 points each]

  Find the derivatives of the following functions.
  \begin{enumerate}
  \item $ f(t)=t $
  \item $ g(x)= e^3 $
  \item $ g(w)= \frac{5}{6}w^{12} $
  \item $ f(s)= \frac{\sqrt{s}}{4} $
  \item $ g(x)= 6x^5-\frac{5}{2}x^2 +x+5$
  \end{enumerate}

  
\end{q}


