\begin{q} % 4.4
  Consider the function $ f(x)=\dfrac{x^4+5x^2}{x^2-16}
  $. \emph{(4 points each)}
  \begin{enumerate}
  \item Describe the intervals where $ f $ in increasing,
    where $ f $ is decreasing, and list the local
    extrema. \vfill
  \item Describe the intervals where $ f $ is concave up and
    those where $ f $ is concave down. \vfill
  \item Graph the function of $ f $. Label all asymptote and
    extrema.\vfill
  \end{enumerate}
\end{q}
\pagebreak

\begin{q} % quiz 13, 3.4, 3.7, 3.9, 3.10
  Compute the derivatives for the following functions:
  \emph{(4 points each)} \\
    \begin{enumerate}
    \item $ f(x)= \dfrac{3e^x-3x}{2\sin(x)+x^2} $ \vfill
    \item $ f(x)= \arcsin (\ln (x^2)) $ \vfill
    \end{enumerate}
\end{q}
\pagebreak

\begin{q} % quiz 16, 3.10
  Without finding the inverse, evaluate the derivative of
  the inverse of the function $f(x)= 2e^{3x}$ at the point $(2,0)$. \emph{(4 points)} 
\end{q}
\vspace{15em}

\begin{q} % 4.2
  Which of the following are required to be true of a
  function $ f $ for the Mean Value Theorem to hold on an
  interval $ [a,b] $: (fill in all that apply) \emph{(2
    points)} \\
  \begin{multicols}{2}
    \begin{itemize}[label={$ \bigcirc $}]
    \item $ f $ in continuous on $ (a,b) $
    \item $f$ has an inverse on $ (a,b) $ \columnbreak
    \item $f$ is differentiable on $ [a,b] $
    \item $ f(a)=f(b) $ \\
    \end{itemize}
  \end{multicols}
  \noindent
  For the function and interval
  $$f(x)=\frac{1}{(x-9)^2}; \; [2,4]$$ find the points
  guaranteed to exist by the Mean Value Theorem \emph{(4
    points)}
\end{q}
\pagebreak

\begin{q} % quiz 14, 3.8
  Solve $ x^2y+7\cos (y)=2y^{2/3} $ for $ \frac{dy}{dx} $. \emph{(4 points)}
\end{q}
\vfill

\begin{q} % quiz 17, 3.11
  The sides of a cube decreases in length at a rate of 2
  meters per second. At what rate is the volume of the cube
  changing when the sides are 4 metes long? \emph{(4 points)}
\end{q}
\vfill\pagebreak

