% body -------------------------

% question 1
\begin{q}[10 points]

  Compute the following limits:
  \begin{enumerate}
  \item $ \lim\limits_{x \to 3^+} \dfrac{2}{(x-3)^3} $
  \item $ \lim\limits_{x \to 3^-} \dfrac{2}{(x-3)^3} $
  \item $ \lim\limits_{x \to 3} \dfrac{2}{(x-3)^3} $
  \end{enumerate}
  
\end{q}


