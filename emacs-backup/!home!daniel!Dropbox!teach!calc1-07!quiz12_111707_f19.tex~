% question 1
\begin{q}

  Compute the following derivatives
  \emph{(a)} $ f(x)= \dfrac{x^3-4x^2+x}{x-2} $ \hfill
  \emph{(b)} $ y= (2\sqrt{x}-1)(4x+1)^{-1} $ \hfill
  \emph{(c)} $ y= \dfrac{4}{p^3} $ \hfill

  
\end{q}


