% question 1
\begin{q}[4 points]
  Evaluate the derivative of $f(x)=\arcsin(\ln (x))$
\end{q}

\begin{q}[4 points]
  Without finding the inverse, evaluate the derivative of
  the inverse of $f(x)=4e^{10x}$ at the point $(4,0)$.  
\end{q}


