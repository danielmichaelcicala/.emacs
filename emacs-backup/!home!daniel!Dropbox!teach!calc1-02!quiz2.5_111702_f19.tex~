\textbf{Instructions:} Circle your answer. You do not need to
justify your answer.

% question 1
\begin{q}[10 points]
  $$ \lim\limits_{\theta \to \infty} \dfrac{\cos
    (\theta}{\theta^2} $$
  \begin{enumerate}
  \item $ \infty $
  \item $ - \infty $
  \item $ 0 $
  \item $ 1 $
  \end{enumerate}  
\end{q}

\begin{q}
  $$ \lim\limits_{x \to -\infty} 5 + \dfrac{100}{x} +
  \dfrac{( \sin (x^3) )^4}{x^2} $$
  \begin{enumerate}
  \item $ \infty $
  \item $ - \infty $
  \item $ 0 $
  \item $ 5 $
  \item $ 105 $
  \end{enumerate}
\end{q}

\begin{q}
  $$ \lim\limits_{x \to -\infty} 3x^7+x^2 $$
  \begin{enumerate}
  \item $ \infty $
  \item $ - \infty $
  \item $ 0 $
  \item $ 3 $
  \item $ 4 $
  \end{enumerate}
\end{q}

\begin{q}
  $$ \lim\limits_{x \to \infty} \dfrac{9x^3+x^2-5}{3x^4+4x^2} $$
  \begin{enumerate}
  \item $ \infty $
  \item $ - \infty $
  \item $ 0 $
  \item $ 3 $
  \item $ -5 $
  \end{enumerate}
\end{q}

\begin{q}
  $$ \lim\limits_{x \to \infty} \dfrac{\sin (x)}{e^x} $$
  \begin{enumerate}
  \item $ \infty $
  \item $ - \infty $
  \item $ 0 $
  \item $ 1 $
  \item $ e $
  \end{enumerate}
\end{q}

