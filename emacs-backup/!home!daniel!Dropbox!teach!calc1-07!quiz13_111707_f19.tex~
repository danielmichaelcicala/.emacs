% body -------------------------

% question 1
\begin{q}[2 points each

  Find the derivatives for the following functions
  \begin{enumerate}
  \item $ y= \dfrac{x\sin(x)}{1+\cos(x)} $
  \item $ y= \dfrac{\sin (x) + \cos(x)}{e^x} $
  \item $ y= \sec(x)\tan(x) $
  \end{enumerate}
  
\end{q}


