\begin{center}
  \begin{tikzpicture}
    \node (La) at (0,3) {$ La $};
    \node (x) at (1,4) {$ x $};
    \node (Lb) at (2,5) {$ Lb $};
    %
    \node (La') at (0,0) {$ La' $};
    \node (x') at (1,1) {$ x' $};
    \node (Lb') at (2,2) {$ Lb' $};
    %
    \node (La'') at (3,3) {$ La'' $};
    \node (x'') at (4,4) {$ x'' $};
    \node (Lb'') at (5,5) {$ Lb'' $};
    % 
    \node (POa) at (3,0) {$ L(a'+_aa'') $};
    \node (POx) at (4,1) {$ x'+_xx'' $};
    \node (POb) at (5,2) {$ L(b'+_bb'') $};
    %
    \draw [cd]
    (La) edge[] (Lx)
    (Lb) edge[] (Lx)
    (La') edge[] (x')
    (La'') edge[] (Lx'')
    (POa) edge[] (POx)
    (POb) edge[] (POx)
    %
    (La) edge[] (La')
    (La) edge[] (La'')
    (La') edge[] (POa)
    (La'') edge[] (POa)
    %
    (x) edge[] (x')
    (x) edge[] (x'')
    (x') edge[] (POx)
    (x'') edge[] (POx)
    %
    (Lb) edge[] (Lb')
    (Lb) edge[] (Lb'')
    (Lb') edge[] (POb)
    (Lb'') edge[] (POb'');  
\end{tikzpicture}
\end{center}
