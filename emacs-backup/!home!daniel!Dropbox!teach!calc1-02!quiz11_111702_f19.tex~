% body -------------------------

% question 1
\begin{q}

  Find the derivatives for the following functions
  \begin{enumerate}
  \item $ g(x)= 6x-2xe^x $
  \item $ f(x)= \dfrac{2e^x-1}{2e^x+1} $
  \item $ h(w)= \dfrac{w^2-1}{w^2+1} $
  \end{enumerate}
  
\end{q}


