% body -------------------------

% question 1
\begin{q}[10 points]
  For the following functions, determine the location and
  value of the absolute extreme values on the given
  interval, if they exist.
  \begin{enumerate}
  \item $f(x)= \dfrac{x}{(x^2+3)^2}$ on $[-2,2]$
  \item $f(x)= x^4-4x^3+4x^2$ on $[-1,3]$
  \end{enumerate}
\end{q}


