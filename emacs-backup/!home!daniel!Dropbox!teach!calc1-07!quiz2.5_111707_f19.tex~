\textbf{Instructions:} Circle your answer. You do not need to
justify your answer.

% question 1
\begin{q}[2 points]
  $ \lim\limits_{\theta \to \infty} \dfrac{\cos
    (\theta)}{\theta^2} $ \vfill
  
  \begin{center}
    \begin{enumerate*}
    \item $ \infty \qquad $
    \item $ - \infty \qquad $
    \item $ 0 \qquad$
    \item $ 1 \qquad$
    \end{enumerate*}
  \end{center}
  \vfill
\end{q}

\begin{q}[2 points]
  $ \lim\limits_{x \to -\infty} 5 + \dfrac{100}{x} +
  \dfrac{( \sin (x^3) )^4}{x^2} $ \vfill
 
  \begin{center}
    \begin{enumerate*}
    \item $ \infty \qquad$
    \item $ - \infty \qquad$
    \item $ 0 \qquad$
    \item $ 5 \qquad$
    \item $ 105 \qquad$
    \end{enumerate*}
  \end{center}
  \vfill
\end{q}

\begin{q}[2 points]
  $ \lim\limits_{x \to -\infty} 3x^7+x^2 $ \vfill

  \begin{center}
    \begin{enumerate*}
    \item $ \infty \qquad$
    \item $ - \infty \qquad$
    \item $ 0 \qquad$
    \item $ 3 \qquad$
    \item $ 4 \qquad$
    \end{enumerate*}
  \end{center}
  \vfill

\end{q}

\begin{q}[2 points]
  $ \lim\limits_{x \to \infty} \dfrac{9x^3+x^2-5}{3x^4+4x^2}
  $ \vfill

  \begin{center}
    \begin{enumerate*}
    \item $ \infty \qquad$
    \item $ - \infty \qquad$
    \item $ 0 \qquad$
    \item $ 3 \qquad$
    \item $ -5 \qquad$
    \end{enumerate*}
  \end{center}
  \vfill

\end{q}

\begin{q}[2 points]
  $ \lim\limits_{x \to \infty} \dfrac{\sin (x)}{e^x} $ \vfill

  \begin{center}
    \begin{enumerate*}
    \item $ \infty \qquad$
    \item $ - \infty \qquad$
    \item $ 0 \qquad$
    \item $ 1 \qquad$
    \item $ e \qquad$
    \end{enumerate*}
  \end{center}
  \vfill

\end{q}

\begin{q}[2 points]
  $ \lim\limits_{x \to -\infty} e^x\cos (x)+3 $ \vfill

  \begin{center}
    \begin{enumerate*}
    \item $ \infty \qquad$
    \item $ - \infty \qquad$
    \item $ 0 \qquad$
    \item $ 3 \qquad$
    \item $ e \qquad$
    \end{enumerate*}
  \end{center}
  \vfill

\end{q}

