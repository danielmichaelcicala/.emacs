% question 1
\begin{q}[4 points each]
  Calculate the derivatives of the following functions: \vspace{2em}
  \emph{(a)} $ y= \sqrt[3]{x^2+9} $ \hfill
  \emph{(b)} $ y= \sin ( 4 \cos (z) ) $ \hfill
  \emph{(c)} $ y= x(x+1)^{1/3} $ \hfill

\end{q}


